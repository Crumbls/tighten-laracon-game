{
    "name": "crumbls/laraconman",
    "description": "A web-based JavaScript Pacman clone for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chase Miller",
            "email": "chase@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Crumbls\\Laraconman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crumbls\\Laraconman\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crumbls\\Laraconman\\LaraconmanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}